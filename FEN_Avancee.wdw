     name : GetNameSpace
     internal_properties : 
     procedure_id : 1487592142428303680
     type_code : 14
     code : |1-
      // Syntaxe :
      //[ <Résultat> = ] GetNameSpace (<sNomArbre> est chaîne, <sNameScope> est chaîne, <sNoeudParent> est chaîne [, <bDernierNoeud> est booléen])
      //
      // Paramètres :
      //	sNomArbre (chaîne ANSI) : Le nom de l'arbre dans lequel on doit ajouter les namespaces
      //	sNameScope (chaîne ANSI) : le namespace pour lequel on doit retrouver les fils
      //	sNoeudParent (chaîne ANSI) : Le noeud à partir duquel on doit ajouter les fils (se termine obligatoirement par TAB)
      //	bDernierNoeud (booléen - valeur par défaut=0) : Est ce le dernier noeud ? (doit on lister tous les fils ou seulement le premier) ==> Optimisation de temps
      // Valeur de retour :
      // 	booléen : VRAi si la récupération est effective, FAUX dans le cas contraire
      //  				On ne liste pas tout d'un coup, on remplit les fils, et le premier fils (s'il y a lieu)
      Procédure GetNameSpace(LOCAL sNomArbre est une chaine, LOCAL sNameScope est une chaine, LOCAL sNoeudParent est une chaine, LOCAL bDernierNoeud est un booleen = faux)
      
      clMesOptions est un ConnectionOptions() // Options de connexion
      pcliEnumObjet est un IEnumerator dynamique // Enumérateur des namespaces
      pclMonObjet est un ManagementObject dynamique // Objet de parcours
      sNoeud est une chaine // Noeud que l'on doit ajouter (Noeud Parent + TAB + Nom du namespace)
      sChemin est une chaine // Chemin du namespace (correspond à l'identifiant dans l'arbre)
      sNomNameSpace est une chaine // Nom du namespace trouvé
      bRes est un booléen // Retour du parcours de l'énumérateur
      
      
      // Création du chemin de recherche
      clMonChemin est un managementpath(sNameScope)
      // Instanciation des options de connexion (par défaut à vide pour le poste local)
      clmonScope est un ManagementScope(clMonChemin, clMesOptions)
      
      // Si on est en mode distant
      si pWMI.gsServeur <> "\\" + NetNomMachine() +"\" ALORS
      	
      	// Mise à jour des options de connexion
      	clMesOptions:Username = pWMI.gsUtilisateur
      	clMesOptions:Password = pWMI.gsPassword
      	
      	clmonScope:set_Options(clMesOptions)
      	
      	// On tente de se connecter
      	quand exception dans
      		clmonScope:connect()
      	faire
      		// Accès refusé
      		erreur(chaineconstruit(<§$0002§>, pwmi.gsServeur))
      		renvoyer faux	
      	fin
      FIN
      
      
      // Commence par récupérer tous les namespaces
      sSearch est une chaine = "SELECT * FROM __namespace"
      
      // Définition de la requête
      clMaRequete est un ObjectQuery(sSearch)
      // Définition de l'objet de recherche
      clChercheur est un ManagementObjectSearcher(clmonScope, clMaRequete)
      
      quand exception dans
      	// Récupération d'un énumérateur
      	pcliEnumObjet = clChercheur:Get():GetEnumerator()
      FAIRE 
      	si EnModeTestAutomatique() = faux alors
      		erreur(<§$001c§>)
      	Fin
      	renvoyer faux
      FIn
      
      // on supprime la branche "non chargé" ajouté lors du chargement dunoeudd père
      Arbresupprime(sNomArbre, sNoeudParent + <§$0004§>)
      
      // Commence le parcours de l'énumération
      bRes = pcliEnumObjet:MoveNext()
      tantque bRes = vrai
      	
      	// Si on doit lister tous les fils
      	si bDernierNoeud = faux ALORS
      		
      		// Récupération de l'objet courant
      		pclMonObjet = pcliEnumObjet:get_Current()
      		
      		// Récupération du nom
      		sChemin = pclMonObjet:get_Path():tostring()
      		
      		// Récupération du nom du namespace
      		sNomNameSpace = pclMonObjet:GetPropertyValue(<§$0005§>)
      		
      		// Construction du noeud
      		sNoeud = sNoeudParent + sNomNameSpace
      		
      		// Ajout du namespace dans l'arbre
      		arbreajoute(sNomArbre, sNoeud, adefaut, aDéfaut, sNameScope + "\" + sNomNameSpace)
      		
      		// liste les fils du namespace en cours (mais en mode dernier noeud : on s'arrête dès qu'on en trouve un)
      		GetNameSpace(sNomArbre, sNameScope + "\" + sNomNameSpace, sNoeud + tab, vrai)
      	sinon
      		
      		// on est en mode dernier noeud
      		// On ajoute le noeud NON CHARGE
      		arbreajoute(sNomArbre, sNoeudParent + <§$0004§>)
      		// on sort de la procédure
      		renvoyer vrai
      	FIN
      	
      	// Passe à l'objet suivant
      	bRes = pcliEnumObjet:MoveNext()
      FIN
      
      renvoyer vrai
     type : 458752
   -
     name : GetClasses
     internal_properties : 
     procedure_id : 1487592142428369216
     type_code : 14
     code : |1+
      // Syntaxe :
      //[ <Résultat> = ] GetClasses (<sNomListe> est chaîne, <sScope> est chaîne [, <sNoeudParent> est chaîne [, <bDernierNoeud> est booléen]])
      //
      // Paramètres :
      //	sNomListe (chaîne ANSI) : Nom de la liste
      //	sScope (chaîne ANSI) : Racine de recherche
      //	sNoeudParent (chaîne ANSI - valeur par défaut="") : Noeud parent
      //	bDernierNoeud (booléen - valeur par défaut=0) : Dernier noeud
      // Valeur de retour :
      // 	booléen : VRAi si la récupération est effective, FAUX dans le cas contraire
      
      Procédure GetClasses(LOCAL sNomListe est une chaine, LOCAL sScope est une chaine, LOCAL sNoeudParent est une chaine = "", LOCAL bDernierNoeud est un booleen = faux)
      
      pclMonObjet est un ManagementObject dynamique // L'objet servant à récupérer un objet de la collection
      pclMonEnum est un IEnumerator dynamique // Enumérateur de classes
      sNoeud est une chaine // Noeud à ajouter dans l'arbre
      clMesOptionsConnection est un ConnectionOptions() // Les options de connections
      sChemin est une chaine // Path de la classe trouvée
      bRes est un booléen
      
      clMesOptionsConnection:Username = pWMI.gsUtilisateur // Nom d'utilisateur
      clMesOptionsConnection:Password = pWMI.gsPassword // Password
      
      // Déclaration du chemin racine de recherche
      clMonChemin est un managementpath(sScope)
      // Déclaration du champ de recherche
      clMonScope est un ManagementScope(clMonChemin, clMesOptionsConnection)
      
      // Récupération d'un objet sur la classe correspondant au chemin racine
      clMaClasse est un ManagementClass(clMonChemin)
      
      // Si on est en mode distant
      si pWMI.gsServeur <> "\\" + NetNomMachine() +"\" ALORS
      	
      	// On tente de se connecter
      	quand exception dans
      		
      		clMonScope:connect()
      		clMaClasse:Scope = clMonScope		
      		
      	faire
      		// Accès refusé
      		erreur(chaineconstruit(<§$0002§>, pWMI.gsServeur), ExceptionInfo())
      		renvoyer faux	
      	fin
      FIN
      
      pclMonEnum = clMaClasse:GetSubclasses():GetEnumerator()
      
      Arbresupprime(sNomListe, sNoeudParent + <§$0004§>)
      
      // Commence l'énumération
      bRes = pclMonEnum:MoveNext()
      
      Tantque bRes = vrai
      
      	// Récupération de l'objet courant	
      	pclMonObjet = pclMonEnum:Current	
      	
      	// Si on n'est pas en mode dernier noeud
      	si bDernierNoeud = Faux ALORS
      
      		// Récupération du nom
      		sChemin = pclMonObjet:get_Path():tostring()
      		
      		// Construction du noeud
      		sNoeud = sNoeudParent + extraitchaine(sChemin, 2, ":")
      		
      		// Ajout dans l'arbre
      		arbreajoute(sNomListe, sNoeud, adefaut, adefaut, sChemin)
      		
      		// Récupération du premier fils, s'il y a lieu
      		GetClasses(sNomListe, sChemin, sNoeud + tab, vrai)
      			
      	sinon
      		// On est en mode dernier noeud, 
      		// il y a au moins un fils, on ajoute le noeud NON CHARGE
      		arbreajoute(sNomListe, sNoeudParent + <§$0004§>)
      		
      		renvoyer faux
      		
      	FIN
      
      	// Passe au suivant
      	bRes = pclMonEnum:MoveNext()
      	
      fIn
      
      renvoyer vrai
     type : 458752
   -
     name : GetPropriétés
     internal_properties : CAAAAAgAAAD4cE0BIW+qQ8axdFLbCs8cc7ap574lxg0sHPD0H59GD/6WBt4dTrxQ9nSVEqIvMwkAEY9aZdlJTMNmomQ87iIkF2N4NRS92SWEY3XJpx9HL4D7jHDcVxgn54QfJ2yVwaQuhS6ugxYGMWHcBxgZG2gRLBqQyfeobxTJNgV9moIu57J8Pq7PnMEs2vc5j4Luty1RCy43GoZ8ItM6cLa5xNarwQKqJv6b
     procedure_id : 1487592142428434752
     type_code : 14
     code : |1+
      // Syntaxe :
      //GetPropriétés (<sObjet> [, <bSystème> est booléen])
      //
      // Cette procédure permet d'ajouter dans la table TABLE_PROPRIETE les propriétés relatives à une classe (ici sObjet)
      // Paramètres :
      //	sObjet : Le nom de la classe dont on doit récupérer les propriétés
      //	bSystème (booléen - valeur par défaut=0) : Propriétés système
      
      Procédure GetPropriétés(LOCAL sObjet,LOCAL bSystème est un booleen= faux)
      
      clMonScope est un ManagementScope(sObjet)
      clMesOptions est un ConnectionOptions
      pcliEnumPropriété est un IEnumerator dynamique // Enumérateur des propriétés
      pclMaPropriété est un PropertyData dynamique // Objet courant
      sType est une chaine // Type de la propriété
      sValeur est une chaine // Valeur de la propriété
      sImage est une chaine // image à ajouter dans la table
      clMaClass est un ManagementClass(sObjet) // Récupération d'un objet sur la classe sélectionnée
      
      
      // Si on est en mode distant, on met à jour les options de connexion
      si pwmi.gsServeur <> "\\" + netnommachine() + "\" ALORS
      	
      	// Définition des options de connexion
      	clMesOptions:Password = pwmi.gsPassword
      	clMesOptions:Username = pwmi.gsUtilisateur
      	// Mise à jour du champ de recherche
      	clMonScope:set_Options(clMesOptions)	
      	
      FIN
      
      // La classe fait partie de ce champ de recherche
      clMaClass:Scope = clMonScope
      
      si bSystème = faux
      	// Récupération d'un énumérateur
      	pcliEnumPropriété = clMaClass:get_Properties():getEnumerator()
      sinon
      	pcliEnumPropriété = clMaClass:get_SystemProperties():GetEnumerator()
      fin
      
      // Commence l'énumération
      Tantque pcliEnumPropriété:MoveNext()
      	
      	// Récupération de l'objet courant
      	pclMaPropriété = pcliEnumPropriété:Current
      	
      	// Vérification pour savoir si c'est un tableau
      	si pclMaPropriété:IsArray ALORS
      		sType = <§$0007§>		
      		
      		sValeur = <§$0008§>
      	sinon
      		quand Exception dans 
      			// Récupération de la valeur de la propriété
      			sValeur = pclMaPropriété:get_Value()		
      		faire
      			sValeur = <§$0009§>
      		fin
      	FIN
      	
      	// Récupération du type de la valeur
      	sType += " " 
      	selon pclMaPropriété:Type
      	
      		cas Cimtype::Boolean
      			sType += <§$000b§>
      		cas Cimtype::Char16
      			sType += <§$000c§>
      		cas Cimtype::DateTime
      			sType += <§$000d§>
      		cas Cimtype::Object
      			sType += <§$000e§>
      		cas Cimtype::Real32
      			sType += <§$000f§>
      		cas Cimtype::Real64
      			sType += <§$0010§>
      		cas Cimtype::Reference
      			sType += <§$0011§>
      		cas Cimtype::SInt16
      			sType += <§$0012§>
      		cas Cimtype::SInt32
      			sType += <§$0013§>
      		cas Cimtype::SInt64
      			sType += <§$0014§>
      		cas Cimtype::uInt16
      			sType += <§$0012§>
      		cas Cimtype::uInt32
      			sType += <§$0013§>
      		cas Cimtype::uInt64
      			sType += <§$0014§>
      		cas Cimtype::uInt8
      			sType += <§$0015§>
      		cas CimType::String
      			sType += <§$000c§>		
      		autre cas 
      			sType += <§$0016§>
      	FIN
      	
      	// Si on liste les propriétés systèmes
      	si bSystème alors
      		
      		sImage = IMG_SYSTEM
      	sinon
      		// Mise à jour de l'image selon le type de propriété
      		si pclMaPropriété:IsLocal ALORS
      			// Propriété Locale
      			sImage = IMG_LOCAL		
      		sinon
      			// Propriété héritée
      			sImage = IMG_HERITAGE	
      		fin	
      			
      	FIN
      	
      	
      	// Ajout de la propriété dans la table
      	tableajouteligne(TABLE_Propriété, sImage, pclMaPropriété:Name, sType, sValeur)
      	multitache(-1)
      	sType = ""
      FIN
     type : 458752
