           code : |1+
            // Vérification de l'ordinateur à parcourir
            SI SAI_SERVEUR <> "\\" + NetNomMachine() ET SAI_SERVEUR <> NetAdresseIP() ALORS
            	
            	// Vérification pour savoir si c'est une adresse IP
            	SI EstAdresseIP(SAI_SERVEUR) ALORS	
            		Erreur(<§$0027§>)
            		RepriseSaisie(SAI_SERVEUR)		
            	FIN
            	
            	SI SAI_SERVEUR[[ A 2]] <> "\\" ALORS
            		SAI_SERVEUR = "\\" + SAI_SERVEUR
            	FIN
            	
            	
            SINON
            	
            	sai_user = ""
            	SAI_PASS = ""	
            	
            FIN
            
            // Mise à jour des paramètres de connexion
            pWMI.gsPassword = SAI_PASS
            pWMI.gsServeur = ComplèteRep(SAI_SERVEUR)
            pWMI.gsUtilisateur = SAI_USER
            
            // Remet à zéro les champs
            ArbreSupprimeTout(ARBRE_NAMESPACE)
            ArbreSupprimeTout(ARBRE_CLASSES)
            TableSupprimeTout(TABLE_Propriété)
            
            Sablier(Vrai)
            
            
            SI pWMI.gsServeur = "\\" + NetNomMachine() + "\" ALORS
            	// Ajout de la racine
            	ArbreAjoute(ARBRE_NAMESPACE, <§$0028§>, aDéfaut, aDéfaut, pWMI.gsServeur + <§$0028§>)
            FIN
            
            // Récupération des namespaces
            GetNameSpace(ARBRE_NAMESPACE..Nom, pWMI.gsServeur + <§$0028§>, <§$0028§> + tab)
            
            // Déroule la racine
            ArbreDéroule(ARBRE_NAMESPACE, <§$0028§>)
            
            Sablier(Faux)
            
            // Rend actif le bouton de visualisation du contenu
            BTN_Voir_le_contenu..Etat = Actif
