  procedures :
   -
     name : PrepareTable
     procedure_id : 1487592473142108391
     type_code : 14
     code : |1+
      // Syntaxe :
      //PrepareTable ()
      //
      // Cette procédure permet de préparer la table de visualisation en créant autant de colonne que de propriété dans la classe à visualiser
      
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      
      Procédure PrepareTable()
      
      nBorne est un entier
      
      
      // Récupère le nombre de ligne de la table
      nBorne = TableOccurrence(FEN_Avancée.TABLE_Propriété)
      
      // Récupération du nombre de colonnes
      pour i = 2 a nBorne
      	
      	// Clone les champs
      	Champclone(TABLE_CONTENU.Colonne1,<§$0001§>+i)
      	
      	// Mise à jour du libellé de la colonne
      	{<§$0001§> + i,indchamp}..Libellé = FEN_Avancée.TABLE_Propriété.COL_Nom[i]
      	
      FIN
     type : 458752
   -
     name : RempliTable
     procedure_id : 1487592473142173927
     type_code : 14
     code : |1+
      // Syntaxe :
      //RempliTable ()
      //
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      Procédure RempliTable()
      
      sListeContenu est une chaine // Contenu de la classe à visualiser
      sUneLigne est une chaine // Une instance de la classe à visualiser
      sUnePropriété est une chaine // Chaine représentant une propriété (Nom + TAB + Valeur)
      nColonne est un entier // Numéro de la colonne correspondante à la propriété en cours
      nLigne est un entier // Ligne en cours de modification
      
      
      // Récupération du contenu de la classe sClasse
      sListeContenu = RécupèreContenu(gsClasse, gsPath)
      
      // Parcours de la liste des instances
      POUR TOUTE CHAINE sUneLigne DE sListeContenu SEPAREE PAR RC+RC
      	
      	// Ajout d'une ligne dans la table de visualisation
      	nLigne = Tableajouteligne(TABLE_CONTENU)
      	
      	// Parcours des propriétés	
      	POUR TOUTE CHAINE sUnePropriété DE sUneLigne SEPAREE PAR RC
      		
      		// Recherche de la colonne dans laquelle on doit mettre la valeur de la propriété	
      		nColonne = tablecherche(FEN_Avancée.COL_Nom, extraitchaine(sUnePropriété, 1))
      		si nColonne > 0 ALORS
      			// On a trouvé une colonne, on met à jour la propriété
      			{<§$0002§>+nColonne, indchamp}[nLigne] = extraitchaine(sUnePropriété, 2)
      			
      		FIN
      	FIN
      	
      FIN
     type : 458752
