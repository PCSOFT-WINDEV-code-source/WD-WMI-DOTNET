  p_codes :
   -
     code : |1+
      procedure fen_contenu(gsClasse est une chaine, gsPath est une chaine)
      
      // Modifie le titre de la fenêtre
      moimeme..titre = chaineconstruit(<§$0000§>, gsClasse)
      
      // Prépare la table (clonage de colonnes) pour afficher les valeurs
      PrepareTable()
      
   -
     code : |1-
      // Remplit la table avec les valeurs
      sablier(vrai)
      RempliTable()
      sablier(faux)
      
      // Si la table est vide, affiche un message
      si TABLE_CONTENU..occurrence = 0 ALORS
      	Info("Aucun contenu pour cette classe")
      	ferme()
      FIN
