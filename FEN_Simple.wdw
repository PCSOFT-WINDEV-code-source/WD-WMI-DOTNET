   -
     name : InfosRéseau
     procedure_id : 1487592584812725113
     type_code : 14
     code : |1-
      // Syntaxe :
      //[ <Résultat> = ] InfosRéseau ()
      //
      // Cette procédure permet de récupérer les informations sur le réseau et de les mettre en forme
      // Elle renvoie une chaîne mise en forme pour l'affichage dans l'exemple
      
      Procédure InfosRéseau()
      
      sChaineInfoTemp est une chaine
      sChaineAnalyse est une chaine
      sUneInfoTemp est une chaine
      
      sChaineInfoTemp = RécupèreInfoRéseau()
      // 	chaîne :libellé + TAB + Description + TAB + Adresse MAC+ TAB + Nom DNS
      
      // Titre de la partie
      sChaineAnalyse += RC + TAB + <§$000b§>
      sChaineAnalyse += RC + rc
      
      // Parcours des résultats et mise à jour de la chaîne de retour
      pour TOUTE CHAINE sUneInfoTemp DE sChaineInfoTemp SEPAREE PAR RC
      	SI sansespace(ExtraitChaîne(sUneInfoTemp, 1)) <> "" ALORS
      		
      		// Mise à jour de la chaîne de retour
      		sChaineAnalyse += ChaîneConstruit(<§$000c§>, extraitchaine(sUneInfoTemp, 1), extraitchaine(sUneInfoTemp, 2))
      		sChaineAnalyse += RC
      		si pas (sansespace(extraitchaine(sUneInfoTemp, 3)) dans ("",eot)) alors
      			sChaineAnalyse += ChaîneConstruit(<§$000d§>, extraitchaine(sUneInfoTemp, 3))
      			sChaineAnalyse += RC
      		Fin
      		SI PAS (SansEspace(ExtraitChaîne(sUneInfoTemp, 4)) DANS ("",EOT)) ALORS
      			sChaineAnalyse += ChaîneConstruit(<§$000e§>, (extraitchaine(sUneInfoTemp, 4) = 0 ? "n/a" SINON extraitchaine(sUneInfoTemp, 4)))
      			sChaineAnalyse += RC + RC
      		Fin
      		
      	FIN	
      FIN
      
      renvoyer sChaineAnalyse
