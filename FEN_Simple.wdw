   -
     name : InfosLecteursDisques
     internal_properties : CAAAAAgAAABAqJXZg28yz/5pBIbXWlMMY74Fw1E+zB3cXMB0Xw8mH65mtt7N/lxw9mTVQhIPIwmg8Y+KRdn5XJOmcuSc3mL0B7Oo9URtGWVkU7W5N1/3z8B77Ni077BPL5wX36RNiezmzeaWe65eGWmUn9Cxc7AZhOJoMe9gJzwxrn31ctq2rwrkFrYH9Fm0og+Rh1omPztIfZBdhFhO9OUE2ojD7mg5u2oCXrYj
     procedure_id : 1487592584812594041
     type_code : 14
     code : |1-
      // Syntaxe :
      //[ <Résultat> = ] InfosLecteursDisques ()
      //
      // Cette procédure permet de récupérer les informations des lecteurs installés et de les mettre en forme
      // Elle renvoie une chaîne mise en forme pour l'affichage dans l'exemple
      
      Procédure InfosLecteursDisques()
      
      
      sChaineInfoTemp est une chaine
      sChaineAnalyse est une chaine
      sUneInfoTemp est une chaine
      
      sChaineInfoTemp = RécupèreInfoLecteurs()
      // Lettre de lecteur + TAB + Description +TAB + Nom de Volume + TAB + Système de fichier + TAB + Espace Total +TAB+
      //  Espace Dispo + TAB + Provider Name
      
      
      // Titre de la partie
      sChaineAnalyse += RC + tab +<§$0002§>
      sChaineAnalyse += RC + rc
      
      // Parcours de la chaine de retour pour récupérer les informations des différents disques
      POUR TOUTE CHAINE sUneInfoTemp DE sChaineInfoTemp SEPAREE PAR RC
      	SI ExtraitChaîne(sUneInfoTemp, 1) <> "" ALORS 
      		
      		// Récupération de la description du lecteur, de la lettre de lecteur, et du nom de volume
      		sChaineAnalyse += ChaîneConstruit(<§$0003§>, ExtraitChaîne(sUneInfoTemp, 2), ExtraitChaîne(sUneInfoTemp, 1), "[" + (ExtraitChaîne(sUneInfoTemp, 3) = eot ? "n/a" sINON ExtraitChaîne(sUneInfoTemp, 3)) +"]")
      		
      		// Récupération de la taille libre et de la taille totale
      		SI PAS (SansEspace(ExtraitChaîne(sUneInfoTemp, 6)) DANS ("", EOT)) ALORS
      			sChaineAnalyse += TAB
      			sChaineAnalyse += ChaîneConstruit(<§$0006§>, tailleverschaine(ExtraitChaîne(sUneInfoTemp, 6)), tailleverschaine((ExtraitChaîne(sUneInfoTemp, 5))))
      		FIN
      		
      		sChaineAnalyse += RC
      		// Récupération du chemin UNC (dans le cas d'un lecteur réseau)
      		SI PAS (sansespace(ExtraitChaîne(sUneInfoTemp, 7)) DANS ("", EOT)) ALORS
      			
      			sChaineAnalyse += ChaîneConstruit(<§$0007§>, (ExtraitChaîne(sUneInfoTemp, 7) = 0 ? "n/a" SINON ExtraitChaîne(sUneInfoTemp, 7)))	
      			sChaineAnalyse += RC 
      		FIN
      	FIN
      	
      	sChaineAnalyse += RC 
      FIN
      
      
      renvoyer sChaineAnalyse
     type : 458752
