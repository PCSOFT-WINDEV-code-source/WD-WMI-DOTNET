   -
     name : InfosUtilisateurs
     procedure_id : 1487592584812856185
     type_code : 14
     code : |1-
      // Syntaxe :
      //[ <Résultat> = ] InfosUtilisateurs ()
      //
      // Cette procédure permet de récupérer les informations sur les utilisateurs et de les mettre en forme
      // Elle renvoie une chaîne mise en forme pour l'affichage dans l'exemple
      
      Procédure InfosUtilisateurs()
      
      
      nIndice est un entier
      sChaineInfoTemp est une chaine
      sChaineAnalyse est une chaine
      sUneInfoTemp est une chaine
      
      sChaineInfoTemp = RécupèreInfoUtilisateurs()
      // 	chaîne :libellé + TAB + Description + TAB + Adresse MAC+ TAB + Nom DNS
      
      // Titre de la partie
      sChaineAnalyse += RC + TAB + <§$0019§>
      sChaineAnalyse += RC + rc
      
      // Parcours des résultats et mise à jour de la chaîne de retour
      nIndice = 1
      POUR TOUTE CHAINE sUneInfoTemp DE sChaineInfoTemp SEPAREE PAR RC
      	
      	SI sansespace(ExtraitChaîne(sUneInfoTemp, 1)) <> "" ALORS
      		// Mise à jour de la chaîne de retour
      		sChaineAnalyse += ChaîneConstruit("%1°) %2 (%3)", nIndice, extraitchaine(sUneInfoTemp, 1), (extraitchaine(sUneInfoTemp, 2) = EOT ? "n/a" SINON extraitchaine(sUneInfoTemp,2)))
      		sChaineAnalyse += RC + RC
      		nIndice ++
      	FIN
      	
      FIN
      
      
      renvoyer sChaineAnalyse
