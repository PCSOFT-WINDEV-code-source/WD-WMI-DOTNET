     name : InfosPartages
     procedure_id : 1487592584812659577
     type_code : 14
     code : |1-
      // Syntaxe :
      //[ <Résultat> = ] InfosPartages ()
      //
      // Cette procédure permet de récupérer les informations des partages et de les mettre en forme
      // Elle renvoie une chaîne mise en forme pour l'affichage dans l'exemple
      
      Procédure InfosPartages()
      
      
      
      sChaineInfoTemp est une chaine
      sChaineAnalyse est une chaine
      sUneInfoTemp est une chaine
      
      sChaineInfoTemp = RécupèrePartages()
      // 	chaîne :libellé + TAB + Description + TAB + Adresse MAC+ TAB + Nom DNS
      
      // Titre de la partie
      sChaineAnalyse += RC + TAB + <§$0008§>
      sChaineAnalyse += RC + rc
      
      // Parcours des résultats et mise à jour de la chaîne de retour
      pour TOUTE CHAINE sUneInfoTemp DE sChaineInfoTemp SEPAREE PAR RC
      	SI sansespace(ExtraitChaîne(sUneInfoTemp, 1)) <> "" ALORS
      		
      		// Mise à jour de la chaîne de retour
      		sChaineAnalyse += ChaîneConstruit(<§$0009§>, extraitchaine(sUneInfoTemp, 1), extraitchaine(sUneInfoTemp, 2))
      		sChaineAnalyse += RC
      		sChaineAnalyse += ChaîneConstruit(<§$000a§>, (extraitchaine(sUneInfoTemp, 3) = eot ? "n/a" SINON extraitchaine(sUneInfoTemp, 3)))
      		sChaineAnalyse += RC + RC
      		
      	FIN	
      FIN
      
      
      renvoyer sChaineAnalyse
