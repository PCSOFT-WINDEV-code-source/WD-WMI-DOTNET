   -
     name : InfosGroupes
     procedure_id : 1487592584812528505
     type_code : 14
     code : |1+
      // Syntaxe :
      //[ <Résultat> = ] InfosGroupes ()
      
      // Cette procédure permet de récupérer les informations sur les groupes définis et de les mettre en forme
      // Elle renvoie une chaîne mise en forme pour l'affichage dans l'exemple
      
      Procédure InfosGroupes()
      
      
      nIndice est un entier
      sChaineInfoTemp est une chaine
      sChaineAnalyse est une chaine
      sUneInfoTemp est une chaine
      
      sChaineInfoTemp = RécupèreInfoGroupes()
      // 	chaîne :libellé + TAB + Description + TAB + Adresse MAC+ TAB + Nom DNS
      
      // Titre de la partie
      sChaineAnalyse += RC + TAB + <§$0000§>
      sChaineAnalyse += RC + rc
      
      nIndice = 1
      // Parcours des résultats et mise à jour de la chaîne de retour
      POUR TOUTE CHAINE sUneInfoTemp DE sChaineInfoTemp SEPAREE PAR RC
      	
      	SI ExtraitChaîne(sUneInfoTemp, 1) <> "" ALORS
      		
      		// Mise à jour de la chaîne de retour
      		sChaineAnalyse += ChaîneConstruit("%1°) %2 (%3)", nIndice, extraitchaine(sUneInfoTemp, 1), (extraitchaine(sUneInfoTemp, 2) = EOT ? "n/a" SINON ExtraitChaîne(sUneInfoTemp, 2)))
      		sChaineAnalyse += RC + RC
      		
      		nIndice++
      	FIN	
      	
      FIN
      
      renvoyer sChaineAnalyse
