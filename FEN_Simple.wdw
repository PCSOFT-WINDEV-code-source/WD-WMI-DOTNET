           code : |1+
            // Si le serveur indiqué ne correspond pas à la machine locale
            SI SAI_SERVEUR <> "\\" + NetNomMachine() ALORS
            	
            	// Mémorise les informations de la machine à interroger
            	pWMI.gsPassword = SAI_PASS
            	pWMI.gsServeur = ComplèteRep(SAI_SERVEUR)
            	pWMI.gsUtilisateur = SAI_USER
            	
            FIN
            
            sChaineAnalyse est une chaîne
            sChaineInfoTemp est une chaîne
            
            Sablier(Vrai)
            
            // mise à jour du titre
            sChaineAnalyse = ChaîneConstruit(<§$0029§>, remplace(SAI_SERVEUR, "\\", ""))
            sChaineAnalyse += RC +RC
            // -----------------------------
            // Récupération des informations sur l'OS
            // -----------------------------
            Multitâche(-1)
            sChaineInfoTemp = InfosSystemeExploitation()
            
            SI sChaineInfoTemp = "" ALORS RETOUR
            
            sChaineAnalyse += [RC] + sChaineInfoTemp
            
            // -----------------------------
            // Récupération des informations sur les disques
            // -----------------------------
            Multitâche(-1)
            sChaineInfoTemp = InfosLecteursDisques()
            
            
            SI sChaineInfoTemp = "" ALORS 
            	SAI_RESUME = sChaineAnalyse
            	RETOUR 
            FIN
            
            sChaineAnalyse += [RC] + sChaineInfoTemp
            
            // -----------------------------
            // Récupération des informations sur les partages
            // -----------------------------
            Multitâche(-1)
            sChaineInfoTemp = InfosPartages()	
            
            
            SI sChaineInfoTemp = "" ALORS 
            	SAI_RESUME = sChaineAnalyse
            	RETOUR 
            FIN
            
            sChaineAnalyse += [RC] + sChaineInfoTemp
            
            // -----------------------------
            // Récupération des informations Réseau 
            // -----------------------------
            Multitâche(-1)
            sChaineInfoTemp = InfosRéseau()
            
            SI sChaineInfoTemp = "" ALORS 
            	SAI_RESUME = sChaineAnalyse
            	RETOUR 
            FIN
            
            sChaineAnalyse += [RC] + sChaineInfoTemp
            // -----------------------------
            // Récupération des informations sur les groupes locaux
            // -----------------------------
            Multitâche(-1)
            sChaineInfoTemp = InfosGroupes()
            
            SI sChaineInfoTemp = "" ALORS 
            	SAI_RESUME = sChaineAnalyse
            	RETOUR 
            FIN
            
            sChaineAnalyse += [RC] + sChaineInfoTemp
            
            // -----------------------------
            // Récupération des informations sur les utilisateurs locaux
            // -----------------------------
            Multitâche(-1)
            sChaineAnalyse += [RC] + InfosUtilisateurs()
            
            SAI_RESUME = sChaineAnalyse
            
            Sablier(Faux)
